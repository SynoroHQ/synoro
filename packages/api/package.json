{
  "name": "@synoro/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun with-env bun run --watch src/server.ts",
    "start": "bun with-env bun run src/server.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "test": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest --logLevel=info",
    "test:run": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest run",
    "test:coverage": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest run --coverage",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@hono/trpc-server": "^0.4.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.2",
    "@opentelemetry/sdk-node": "^0.204.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@sindresorhus/slugify": "2.2.1",
    "@synoro/auth": "workspace:*",
    "@synoro/db": "workspace:*",
    "@synoro/lib": "workspace:*",
    "@synoro/prompts": "workspace:*",
    "@synoro/validators": "workspace:*",
    "@t3-oss/env-core": "0.13.8",
    "@trpc/server": "^11.5.0",
    "ai": "5.0.32",
    "date-fns": "4.1.0",
    "langfuse": "^3.38.5",
    "langfuse-vercel": "^3.38.5",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "superjson": "2.2.2",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@synoro/eslint-config": "workspace:*",
    "@synoro/prettier-config": "workspace:*",
    "@synoro/tests": "workspace:*",
    "@synoro/tsconfig": "workspace:*",
    "eslint": "9.34.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "prettier": "@synoro/prettier-config"
}
