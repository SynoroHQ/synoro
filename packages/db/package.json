{
  "name": "@synoro/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./client": {
      "default": "./dist/client.js"
    },
    "./client-ws": {
      "default": "./dist/client-ws.js"
    },
    "./schema": {
      "default": "./dist/schema.js"
    },
    "./types": {
      "default": "./dist/types.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "seed": "bun run with-env -- bun run src/seed/index.ts",
    "seed:customers": "bun run with-env -- bun run src/seed/index.ts customers",
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "clear-brands": "bun run with-env -- bun run src/scripts/delete-brands.ts",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "generate": "bun run with-env -- drizzle-kit generate",
    "migrate": "bun run with-env -- drizzle-kit migrate",
    "push": "bun run with-env -- drizzle-kit push",
    "reset-db": "bun run with-env -- bun run src/scripts/reset-db.ts",
    "studio": "bun run with-env -- drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "2.2.1",
    "@t3-oss/env-core": "0.13.8",
    "axios": "^1.11.0",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "file-type": "^21.0.0",
    "pg": "^8.16.3",
    "zod": "4.1.3"
  },
  "devDependencies": {
    "@synoro/eslint-config": "workspace:*",
    "@synoro/prettier-config": "workspace:*",
    "@synoro/tsconfig": "workspace:*",
    "@types/bun": "^1.2.21",
    "@types/pg": "^8.15.5",
    "dotenv-cli": "10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "9.34.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "prettier": "@synoro/prettier-config"
}
